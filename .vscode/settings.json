{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "notebook.formatOnCellExecution": true,
  "notebook.formatOnSave.enabled": true,
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "files.autoSave": "afterDelay",
  "editor.wordWrap": "on",
  "editor.codeActions.triggerOnFocusChange": true,
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.defaultColorDecorators": "auto",
  "editor.definitionLinkOpensInPeek": true,
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "terminal.integrated.enableVisualBell": true,
  "terminal.integrated.tabs.defaultColor": "terminal.ansiCyan",
  "editor.cursorStyle": "line",
  "editor.hideCursorInOverviewRuler": true,
  "editor.overtypeCursorStyle": "line",
  "editor.cursorBlinking": "expand",
  "powermode.presets": "flames",
  "powermode.enabled": true,
  "powermode.combo.timerEnabled": "hide",
  "powermode.shake.enabled": false,
  "powermode.shake.intensity": 0,
  "powermode.explosions.enabled": true,
  "powermode.combo.counterSize": 0,
  "errorLens.codeLensEnabled": false,
  "editor.showUnused": false,
  "intelephense.diagnostics.unusedSymbols": false,
  "editor.fontFamily": "Operator Mono",
  "notebook.defaultFormatter": "esbenp.prettier-vscode",
  "markdown.validate.unusedLinkDefinitions.enabled": "ignore",
  "notebook.output.minimalErrorRendering": true,
  "editor.codeActionWidget.includeNearbyQuickFixes": true,
  "eslint.format.enable": true,
  "terminal.integrated.cursorStyleInactive": "line",
  "terminal.integrated.cursorWidth": 2,
  "editor.cursorSmoothCaretAnimation": "explicit"
}
